{
  "name": "react-clear-cache",
  "version": "1.4.12",
  "description": "A component to manage application updates.",
  "author": "noahjohn9259",
  "license": "MIT",
  "repository": "noahjohn9259/react-clear-cache",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "bin": {
    "generate-build-meta": "./bin/cli.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "release:major": "npm version major && git push origin && git push origin --tags",
    "release:minor": "npm version minor && git push origin && git push origin --tags",
    "release:patch": "npm version patch && git push origin && git push origin --tags"
  },
  "dependencies": {
    "minimist": "^1.2.5",
    "use-persisted-state": "^0.3.0",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/use-persisted-state": "^0.3.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "react-scripts": "^3.3.1",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-url": "^1.4.0",
    "typescript": "3.5.2"
  }
}
